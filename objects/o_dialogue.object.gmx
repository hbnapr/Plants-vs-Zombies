            <string>///Draw text on screen

//Get the next line from the queue
if(line == "") {
    if(!ds_queue_empty(global.script)) {
        line = ds_queue_dequeue(global.script);
        alarm[0] = 150;
    }
}

//Calculate the middle view
//This one needs scaling
middle_x = (view_wview[view_current] / 2) * global.gui_scale_x;
middle_y = (view_hview[view_current] / 2) * global.gui_scale_y;

//Set the font
draw_set_font(f_bigger_shinier_impact);

//Set the text alignment
draw_set_halign(fa_center);
draw_set_valign(fa_middle);

//Write the text twice, once for its shadow, once more for its foreground color
for(i = 0; i &lt; 2; i++) {

    //Black the first time, red the second time
    if(i == 0) { draw_set_color(c_black); } else { draw_set_color(c_red); }

    //Draw the shadow text
    draw_text_ext
    (
        middle_x - (i + 2),
        middle_y - (i + 2),
        line,
        font_get_size(f_bigger_shinier_impact),
        (view_wview[view_current] * global.gui_scale_x) - 150
    );
    
}

</string>
